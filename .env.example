# New Relic Configuration
NEW_RELIC_LICENSE_KEY=your_license_key_here
NEW_RELIC_ACCOUNT_ID=your_account_id
NEW_RELIC_API_KEY=your_api_key_here
NEW_RELIC_REGION=US

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DATABASE=testdb
POSTGRES_SSL_MODE=disable

# Collector Configuration
COLLECTION_INTERVAL_SECS=30
COLLECTOR_MODE=hybrid
DEBUG_MODE=false
RUST_LOG=info
LOG_LEVEL=info

# OpenTelemetry Configuration
OTLP_ENDPOINT=http://localhost:4317
OTLP_COMPRESSION=gzip

# NRI Configuration
NRI_ENTITY_KEY=localhost:5432
NRI_INTEGRATION_NAME=com.newrelic.postgresql

# Extended Metrics
ENABLE_EXTENDED_METRICS=true
ENABLE_ASH=true
ENABLE_EBPF=false

# Query Sanitization
SANITIZE_QUERY_TEXT=true
QUERY_MONITORING_COUNT_THRESHOLD=10
QUERY_MONITORING_RESPONSE_TIME_THRESHOLD=100

# Health Check
HEALTH_CHECK_PORT=8080

# PgBouncer (Optional)
PGBOUNCER_ENABLED=false
PGBOUNCER_ADMIN_CONNECTION=postgresql://pgbouncer@localhost:6432/pgbouncer